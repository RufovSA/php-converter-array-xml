{
    "name": "rufov/converter-array-xml",
    "description": "The class is designed to convert an XML document into a PHP array and vice versa",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "php",
        "component"
    ],
    "homepage": "https://github.com/rufovS",
    "readme": "README.md",
    "time": "2023-02-27",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergej Rufov",
            "email": "me@rufov.ru",
            "homepage": "https://github.com/rufovS",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/rufovS/converter-array-xml"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "spatie/array-to-xml": "^3.1",
        "ext-dom": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "pestphp/pest": "^2.0",
        "spatie/pest-plugin-snapshots": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "RufovS\\ConverterArrayXML\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RufovS\\ConverterArrayXML\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
